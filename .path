# Reset defaults
export PATH=$PATH:/usr/local/bin:/usr/local/sbin:/usr/bin:/bin:/usr/sbin:/sbin:/usr/X11/bin:/sbin

# Postgres.app
export PATH="/Applications/Postgres93.app/Contents/MacOS/bin:$PATH"

# sbin for Homebrew
export PATH="/usr/local/sbin:$PATH"

# Sqlite3
export PATH="/usr/bin/sqlite3:$PATH"

# Python
export PATH="/usr/local/Cellar/python:$PATH"

# NPM
# export PATH=${PATH}:/usr/local/share/npm/bin:

# Ruby Gem Bundler
#export PATH=$PATH:/usr/local/Cellar/ruby193/1.9.3-p448/bin

# Go
export GOPATH=$HOME/golang
export GOROOT=/usr/local/opt/go/libexec
export PATH=$PATH:$GOPATH/bin
export PATH=$PATH:$GOROOT/bin
