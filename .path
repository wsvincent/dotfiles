# Homebrew paths
export PATH=${PATH}:/usr/local/bin:/usr/bin:/bin:/usr/sbin:/sbin:~/bin:$PATH

# Python packages
export PATH=${PATH}:/usr/local/Cellar/python/2.7.5/bin

# Node.js
export PATH=/usr/local/share/npm/bin:$PATH

# Gems
export PATH=${PATH}:/Library/Ruby/Gems/1.8/gems

# bundle bin
export PATH=${HOME}/.rbenv/versions/1.9.3-p392/bin:${PATH}
export PATH=./bin:${PATH}

# Postgresapp
export PATH=/Applications/Postgres.app/Contents/MacOS/bin:$PATH
